@model IEnumerable<EntityLayer.Concrete.Basvuru>
@{
    ViewData["Title"] = "KullaniciRecourseView";
    Layout = "~/Views/Shared/KullaniciAdmin/_KullaniciAdminLogged.cshtml";
}

<section class="cat_product_area section_padding">
    <div class="container">
        <h2 class="text-center text-heading">Your Recourse</h2>
        <div class="table-responsive">
            <table class="table table-striped table-sm">
                <thead>
                    <tr>
                        <th scope="col">Basvuru ID</th>
                        <th scope="col">Category Name</th>
                        <th scope="col">Project Name</th>                        
                        <th scope="col">Position Name</th>
                        <th scope="col">Position Level</th>
                        <th scope="col">Bid</th>
                        <th scope="col">Setting</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>@item.basvuruID</td>
                        @await Component.InvokeAsync("RecourseProjectKategoriList", item.projeID)
                        <td>@item.projeAdi</td>
                        <td>@item.pozisyonAdi</td>
                        <td>@item.pozisyonSeviyesi</td>
                        <td>@item.teklif $</td>
                        <td>
                            <!-- <a href="#" class="view" title="" data-toggle="tooltip" data-original-title="View"><i class="far fa-eye"></i></a>&nbsp; -->
                            <a href="/Admin/KullaniciRecourseEdit/@item.basvuruID" class="edit" title="" data-toggle="tooltip" data-original-title="Edit"><i class="fas fa-pencil-alt"></i></a>&nbsp;
                            <a href="/Admin/KullaniciRecourseDelete/@item.basvuruID/" class="delete" title="" data-toggle="tooltip" data-original-title="Delete"><i class="fas fa-trash-alt"></i></a>
                        </td>
                    </tr>
                    }


                </tbody>
            </table>
        </div>
    </div>
</section>
