@model IEnumerable<EntityLayer.Concrete.Basvuru>
    @{
        ViewData["Title"] = "RecourseView";
        Layout = "~/Views/Shared/KullaniciAdmin/_KullaniciAdminLogged.cshtml";
    }



    <section class="cat_product_area section_padding">
        <div class="container">
            <h2 class="text-center text-heading">Applications</h2>
            <div class="table-responsive">
                <table class="table table-striped table-sm">
                    <thead>
                        <tr>
                            <th scope="col">Recourse ID</th>
                            <th scope="col">Person Name</th>
                            <th scope="col">Project Name</th>
                            <th scope="col">Position Name</th>
                            <th scope="col">Position Level</th>
                            <th scope="col">Bid</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>@item.basvuruID</td>
                            @await Component.InvokeAsync("UyeNameListBasvuru", item.uyeID)
                            <td>@item.projeAdi</td>
                            <td>@item.pozisyonAdi</td>
                            <td>@item.pozisyonSeviyesi</td>
                            <td>@item.teklif $</td>

                        </tr>
                        }


                    </tbody>
                </table>
            </div>
        </div>
    </section>
